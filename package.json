{
  "name": "react-luckydraw",
  "version": "0.1.6",
  "author": "Gemer Cheung <gemercheung@gmail.com>",
  "description": "Simple luckydraw component for React.",
  "main": "lib/index.js",
  "homepage": "http://blog.gemer.xyz/react-luckydraw",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gemerz/react-luckydraw.git"
  },
  "keywords": [
    "react",
    "luckydraw"
  ],
  "license": "ISC",
  "prepublish": "npm run build-lib && npm run build-css",
  "devDependencies": {
    "autoprefixer": "9.6.1",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "connect-history-api-fallback": "1.6.0",
    "cross-spawn": "6.0.5",
    "css-loader": "3.2.0",
    "detect-port": "1.3.0",
    "dotenv": "8.0.0",
    "eslint": "6.1.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "4.1.0",
    "filesize": "4.1.2",
    "fs-extra": "8.1.0",
    "gh-pages": "^2.1.0",
    "gzip-size": "5.1.1",
    "highlight.js": "^9.9.0",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.19.1",
    "jest": "24.8.0",
    "json-loader": "0.5.7",
    "object-assign": "4.1.1",
    "path-exists": "4.0.0",
    "postcss-loader": "3.0.0",
    "prettier": "^1.8.2",
    "promise": "8.0.3",
    "react-dev-utils": "^9.0.1",
    "react-highlight": "^0.12.0",
    "recursive-readdir": "2.2.2",
    "strip-ansi": "5.2.0",
    "style-loader": "1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache": "^5.0.0",
    "url-loader": "2.1.0",
    "webpack": "4.39.1",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "dependencies": {
    "d3": "^5.9.7",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-tabs": "^3.0.0",
    "styled-components": "^4.3.2",
    "sweetalert": "^2.0.8"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && npm run gen-service-worker",
    "format": "prettier --write 'src/**/*.{js,jsx}' '*.{js,jsx}' 'cosmos/**/*.{js,jsx}' --single-quote",
    "test": "node scripts/test.js --env=jsdom",
    "gen-service-worker": "sw-precache --config=sw-precache-config.js",
    "build-lib": "BABEL_ENV=production babel src/LuckyDraw -d lib --ignore spec.js,test.js",
    "build-css": "./node_modules/stylus/bin/stylus -w src/LuckyDraw/Style -o lib/LuckyDraw.css",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
